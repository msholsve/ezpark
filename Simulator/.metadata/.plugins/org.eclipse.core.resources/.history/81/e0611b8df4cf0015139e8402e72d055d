	private boolean keepRunning;
	
	public SeatChangeSimulation() {
		keepRunning = true;
	}
	
	public void stop() {
		keepRunning = true;
	}
	
    public void run() {
        
    	while(keepRunning) {
    		
    	}
    	
    }

}
